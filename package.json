{
  "name": "anonymous-feedback-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db seed && next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:seed": "prisma db seed",
    "build:worker": "opennextjs-cloudflare",
    "preview": "opennextjs-cloudflare && wrangler dev",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["prisma", "@prisma/client"]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.6.0",
    "prisma": "^6.6.0",
    "zod": "^3.24.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.0.0-rc.2+sha512.b6e59b96f90ca92449ac2f6dca9b430880448fc97d21f0fa9200e3d5ddb5289ad535255400554f7f3486e2d60058492161aaa9b58828e81f50c096718be9d156"
}
